{
  "manifest_version": 3,
  "name": "Bookmark Dashboard",
  "version": "0.0.3",
  "description": "Replace your new tab with a customizable bookmark dashboard featuring smart responsive layouts",
  "permissions": [
    "storage",
    "bookmarks"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "options/options.html",
  "action": {
    "default_popup": "options/options.html",
    "default_title": "Bookmark Dashboard Settings"
  }
}